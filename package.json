{
  "name": "rrb-server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js --exec babel-node",
    "test": "npm run compile && mocha --exit ./test/*.js --compilers js:babel-core/register",
    "cover": "nodemon --exec nyc --reporter=lcov --reporter=text-summary mocha  */*.test.js",
